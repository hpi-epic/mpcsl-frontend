{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-react"],
  "jsRules": {
    "jsx-no-multiline-js": true,
  },
  "rules": {
    "quotemark": [true, "single"],
    "no-submodule-imports": false,
    "object-literal-sort-keys": false,
    "semicolon": true,
  },
  "rulesDirectory": []
}

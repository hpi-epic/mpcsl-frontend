  },
  "devDependencies": {
    "@types/d3": "^5.7.2",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.20",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.4",
    "@types/react-lazylog": "^4.4.0",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.3",
